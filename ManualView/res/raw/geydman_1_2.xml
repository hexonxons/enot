        <Tasks>
        	<Task number="2" tasktype="3">
	            <TaskDescription>Выполни действия:</TaskDescription>
	        	<TaskResources>
	        		<TaskResource>2+7=?</TaskResource>
	        		<TaskResource>8-4=?</TaskResource>
	        		<TaskResource>3+6=?</TaskResource>
					<TaskResource>10-6=?</TaskResource>
					<TaskResource>9+1=?</TaskResource>
					<TaskResource>6-3=?</TaskResource>
					<TaskResource>2+8=?</TaskResource>
					<TaskResource>7-5=?</TaskResource>
					<TaskResource>3+3=?</TaskResource>
					<TaskResource>10-7=?</TaskResource>
					<TaskResource>4+5=?</TaskResource>
					<TaskResource>6-2=?</TaskResource>
				</TaskResources>        
		        <TaskAnswer>
		        	<Answer>2+7=9</Answer>
	        		<Answer>8-4=4</Answer>
	        		<Answer>3+6=9</Answer>
	        		<Answer>10-6=4</Answer>
	        		<Answer>9+1=10</Answer>
	        		<Answer>6-3=3</Answer>
	        		<Answer>2+8=10</Answer>
	        		<Answer>7-5=2</Answer>
	        		<Answer>3+3=6</Answer>
	        		<Answer>10-7=3</Answer>
	        		<Answer>4+5=9</Answer>
	        		<Answer>6-2=4</Answer>
	        	</TaskAnswer>
            </Task>
            <Task number="3" tasktype="3">
	            <TaskDescription>У Оли 7 кукол, а у Маши на 3 куклы больше.&#10;Сколько кукол у Маши?</TaskDescription>
	        	<TaskResources>
	        		<TaskResource>?????</TaskResource>
				</TaskResources>        
		        <TaskAnswer>
		        	<Answer>7+3=10&#38;3+7=10</Answer>
	        	</TaskAnswer>
            </Task>
            <Task number="4" tasktype="3">
	            <TaskDescription>Вставь пропущенные знаки действий:</TaskDescription>
	        	<TaskResources>
	        		<TaskResource>5?4?3?2?1=1</TaskResource>
				</TaskResources>        
		        <TaskAnswer>
		        	<Answer>5-4+3-2-1=1</Answer>
	        	</TaskAnswer>
            </Task>
            <Task number="6" tasktype="3">
	            <TaskDescription>Сравни выражения, не вычисляя их значений:</TaskDescription>
	        	<TaskResources>
	        		<TaskResource>5+4?4+5</TaskResource>
	        		<TaskResource>7+3?3+7</TaskResource>
	        		<TaskResource>4+2?4+3</TaskResource>
					<TaskResource>3+5?2+5</TaskResource>
					<TaskResource>7-3?7-2</TaskResource>
					<TaskResource>9-4?8-4</TaskResource>
				</TaskResources>        
		        <TaskAnswer>
		        	<Answer>5+4=4+5</Answer>
	        		<Answer>7+3=3+7</Answer>
	        		<Answer>4+2&#60;4+3</Answer>
	        		<Answer>3+5&#62;2+5</Answer>
	        		<Answer>7-3&#60;7-2</Answer>
	        		<Answer>9-4&#62;8-4</Answer>
	        	</TaskAnswer>
            </Task>
        </Tasks>
